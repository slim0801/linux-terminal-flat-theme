	"tokenColors": [
		{
			"name": "COMMENTS",
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"meta.tag"
			],
			"settings": {
				"foreground": "#6a6a6a"
			}
		},
		{
			"name": "CONSTANTS",
			"scope": [
				"keyword",
				"storage",
				"constant",
				"support",
				"entity.name",
				"meta.property-value",
				"punctuation.definition.entity",
				"punctuation.definition.tag",
			],
			"settings": {
				"foreground": "#881391"
			}
		},
		{
			"name": "VARIABLES",
			"scope": [
				"variable",
				"punctuation.definition.variable",
				"support.other.namespace",
				"support.class",
				"support.type.property-name",
				"meta.property-name",
				"meta.instance.constructor",
				"meta.object-literal.key",
				"entity.other.inherited-class",
				"entity.name.type.class",
				"entity.other.attribute-name",
				
			],
			"settings": {
				"foreground": "#0066cc"
			}
		},
		{
			"name": "VARIABLES 2",
			"scope": [
				"variable.other.property",
				"meta.property.object"
			],
			"settings": {
				"foreground": "#0066cc"
			}
		},
		{
			"name": "PUNCTUATION",
			"scope": [
				"keyword.operator",
				"punctuation",
				"meta.brace",
				"meta.group.braces",
				"text",
				"meta.jsx.children.js"
			],
			"settings": {
				"foreground": "#0000bb"
			}
		},
		{
			"name": "PUNCTUATION 2",
			"scope": [
				"meta.separator",
				"invalid",
			],
			"settings": {
				"foreground": "#0000bb"
			}
		},
		{
			"name": "STRINGS",
			"scope": [
				"keyword.other",
				"entity.name.tag.css",
				"string.quoted",
				"string.template",
				"punctuation.definition.string",
			],
			"settings": {
				"foreground": "#af1212"
			}
		},
		{
			"name": "FUNCTIONS",
			"scope": [
				"variable.function",
				"entity.name.function",
				"meta.function-call"
			],
			"settings": {
				"foreground": "#007700"
			}
		},
		{
			"name": "BUILT-IN FUNCTIONS",
			"scope": [
				"support.function",
				"keyword.operator.expression",
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#007700",
			}
		},
		{
			"name": "BUILT-IN FUNCTIONS 2",
			"scope": [
				"entity.other.attribute-name",
			],
			"settings": {
				"fontStyle": "italic",
			}
		}
	],


}